{
  "name": "gadget-catalog",
  "version": "0.16.0",
  "description": "A react application for cataloging gadgets",
  "author": "Shibbir Ahmed",
  "license": "MIT",
  "scripts": {
    "start": "run-p webpack:development pm2:development",
    "pm2:development": "cross-env NODE_ENV=development pm2 start server.js --watch --no-daemon --name gadget-catalog",
    "webpack:development": "cross-env NODE_ENV=development webpack --progress --watch --stats-error-details",
    "pm2:production": "cross-env NODE_ENV=production pm2 start server.js --name gadget-catalog",
    "webpack:production": "cross-env NODE_ENV=production webpack --progress",
    "production": "run-s webpack:production pm2:production",
    "test": "nyc mocha --color --timeout 10000 --recursive './tests/**/*spec.js'",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "seed": "node seeder.js",
    "eslint": "eslint src/**"
  },
  "dependencies": {
    "@casl/ability": "^5.1.2",
    "@sentry/browser": "^6.0.1",
    "@sentry/node": "^6.0.1",
    "async": "^3.2.0",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.23.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "date-fns": "^2.16.1",
    "dotenv": "^8.2.0",
    "draft-js": "^0.11.7",
    "draft-js-export-html": "^1.4.1",
    "express": "^4.17.1",
    "express-hbs": "^2.3.4",
    "fomantic-ui-css": "^2.8.7",
    "formik": "^2.2.6",
    "glob": "^7.1.6",
    "helmet": "^4.4.1",
    "highcharts": "^8.2.2",
    "izitoast": "^1.4.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "mongoose": "^5.11.13",
    "multer": "^1.4.2",
    "nodemailer": "^6.4.17",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "prop-types": "^15.7.2",
    "query-string": "^6.13.8",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-google-recaptcha": "^2.1.0",
    "react-intl": "^5.10.16",
    "react-redux": "^7.2.2",
    "react-router-bootstrap": "^0.25.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-promise-middleware": "^6.1.2",
    "redux-thunk": "^2.3.0",
    "semantic-ui-react": "^2.0.3",
    "validator": "^13.5.2",
    "yup": "^0.32.8"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "babel-loader": "^8.2.2",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-react-html-attrs": "^3.0.5",
    "chai": "^4.2.0",
    "clean-webpack-plugin": "^3.0.0",
    "coveralls": "^3.1.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "eslint": "^7.18.0",
