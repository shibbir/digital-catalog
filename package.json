    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "pm2": "^4.5.1",
    "supertest": "^6.1.1",
    "terser-webpack-plugin": "^5.1.1",
    "url-loader": "^4.1.1",
    "webpack": "5.16.0",
    "webpack-cli": "4.4.0",
    "webpack-merge": "^5.7.3"
  }
}
