{
  "name": "gadget-catalog",
  "version": "0.10.0",
  "description": "A react application for cataloging gadgets",
  "author": "Shibbir Ahmed",
  "license": "MIT",
  "scripts": {
    "start": "run-p webpack:development pm2:development",
    "pm2:development": "cross-env NODE_ENV=development pm2 start server.js --watch --no-daemon",
    "webpack:development": "cross-env NODE_ENV=development webpack --progress --watch --display-error-details",
    "pm2:production": "cross-env NODE_ENV=production pm2 start server.js",
    "webpack:production": "cross-env NODE_ENV=production webpack --progress",
    "production": "run-s webpack:production pm2:production",
    "test": "nyc mocha --color --timeout 10000 --recursive './tests/**/*spec.js'",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "seed": "node seeder.js"
  },
